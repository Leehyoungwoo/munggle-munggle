{
  "expo": {
    "name": "mungglemunggle",
    "slug": "mungglemunggle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "@react-native-seoul/naver-login",
        {
          "urlScheme": "http://i10a410.p.ssafy.io:8080/oauth2/authorization/naver"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.soEasy.Munggle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.soEasy.Munggle"
    },
    "web": {
      "favicon": "./assets/favicon.ico"
    }
  }
}

